pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh 'echo Sathish is building the code'
      }
    }
    stage('Test') {
      steps {
        sh 'echo Surendra is testing the code'
      }
    }
    stage('Deploy') {
      steps {
        sh 'echo Suman has deployed the code'
      }
    }
  }
}