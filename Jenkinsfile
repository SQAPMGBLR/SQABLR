pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'Sathish is building the code'
      }
    }
    stage('Test') {
      steps {
        echo 'Surendra is testing the code'
      }
    }
    stage('Deploy') {
      steps {
        echo 'Suman has deployed the code'
      }
    }
  }
}